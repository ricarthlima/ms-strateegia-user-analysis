{
    "manifest_version": 3,
    "name": "Strateegia+",
    "description": "This extension will analyze Strateegia's comments.",
    "version": "1.2",
    "action": {
        "default_icon": "icons/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://app.strateegia.digital/"
    ]
}