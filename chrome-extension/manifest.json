{
    "manifest_version": 3,
    "name": "Strateegia User Comments Analysis",
    "description": "This extension will analyze comments from Strateegia.",
    "version": "1.0",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://app.strateegia.digital/"
    ]
}